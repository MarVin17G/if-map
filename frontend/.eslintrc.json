{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb",
        "eslint:recommended",
        "plugin:react-hooks/recommended"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module", 
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "plugins": [
        "react",
        "react-hooks"
    ],
    "rules": {
        "linebreak-style": "off",
        "react/no-typos": "error",
        "react/no-unused-state": "error", 
        "no-dupe-class-members": "error",
        "no-dupe-keys": "error"
    },
    "globals": {
        "process": true
      },
      "settings": {
        "react": {
          "version": "detect"
        }
      }
}
